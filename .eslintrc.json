{
  "extends": "next/core-web-vitals",
  "rules": {
    "react/jsx-sort-props": [
      1,
      {
        "callbacksLast": true,
        "multiline": "last",
        "ignoreCase": false,
        "shorthandFirst": true,
        "noSortAlphabetically": true
      }
    ]
  },
  "overrides": [
    {
      "files": ["src/**/*.js", "src/**/*.jsx"],
      "parserOptions": {
        "project": ["jsconfig.json"]
      }
    }
  ]
}
